{
  "name": "oaklandai",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node backend/index.js",
    "dev": "node backend/index.js",
    "start:all": "node start.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "dev:all": "npm run start:all && pm2 logs",
    "setup": "node -e \"console.log('Setting up environment...'); require('dotenv').config({ path: require('path').join(__dirname, '.env') }); console.log('Environment loaded successfully');\"",
    "check-env": "node check-env.js",
    "db:migrate": "psql -d oaklandai -f database/schema.sql",
    "db:reset": "psql -d oaklandai -c \"DROP SCHEMA public CASCADE; CREATE SCHEMA public;\" && npm run db:migrate",
    "db:setup": "sudo -u postgres psql -f database/setup.sql",
    "init": "node setup.js"
  },
